[default]
ao=pulse,alsa
geometry=50%:50%
ontop=yes
softvol=yes
use-filename-title=yes
vf=screenshot
vo=xv,x11
volume=40
cache=8192

[hd]
lavdopts=fast=1:skiploopfilter=all
vfm=ffmpeg

[hd-low]
lavdopts=fast=1:skiploopfilter=all:lowres=1
vfm=ffmpeg

[reset]
brightness=0
contrast=0
hue=0
saturation=0

[gapless]
ao=pcm:nowaveheader:fast:file=aufifo
vc=null
vo=null
