## Key bindings for ncmpc (generated by ncmpc)

## Key configuration screen
key screen-keyedit = 'K',  '4',  268

## Quit
key quit = 'q',  3,  27

## Move cursor to the top of the list
key home = 262,  'g'

## Move cursor to the bottom of the list
key end = 360,  'G'

## Page up
key pgup = 339,  2

## Page down
key pgdn = 338,  6

## Scroll up one line
key scroll-down-line = 5

## Scroll down one line
key scroll-up-line = 25

## Scroll up half a screen
key scroll-down-half = 4

## Scroll down half a screen
key scroll-up-half = 21

## Select currently playing song
key select-playing = 0

## Play/Enter directory
key play = 13

## Pause
key pause = 32,  'p'

## Seek forward
key seek-forward = 'f',  261

## Seek backward
key seek-backward = 'b',  260

## Increase volume
key volume-up = 43,  '0'

## Decrease volume
key volume-down = 45,  '9'

## Select/deselect song in playlist
key select = 'a'

## Start a music database update
key db-update = 'Q'

## Add url/file to playlist
key add = 'A'

## Go to parent directory
key go-parent-directory = 39

## Locate song in browser
key locate = 'l'

## Backward find previous
key rfind-next = 'N'

